{
    "name": "antonioprimera/efc",
    "description": "efc",
    "authors": [
        {
            "name": "Antonio Primera",
            "email": "antonio@cus.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-simplexml": "*",
        "antonioprimera/anaf-data-structures": "^0.2",
        "antonioprimera/filesystem": "^2.2",
        "spatie/laravel-data": "^4.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\Efc\\": "src/",
            "AntonioPrimera\\Efc\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioPrimera\\Efc\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioPrimera\\Efc\\EfcServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "proprietary"
}
